:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x647045d49dc0 .scope module, "Guia_0903" "Guia_0903" 2 20;
 .timescale 0 0;
v0x647045d953f0_0 .net "clock", 0 0, v0x647045d4b7b0_0;  1 drivers
v0x647045d954e0_0 .net "p", 0 0, v0x647045d952f0_0;  1 drivers
S_0x647045d49f50 .scope module, "clk" "clock" 2 22, 3 4 0, S_0x647045d49dc0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "clk";
v0x647045d4b7b0_0 .var "clk", 0 0;
S_0x647045d950b0 .scope module, "pls" "pulse" 2 25, 2 8 0, S_0x647045d49dc0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "signal";
    .port_info 1 /INPUT 1 "clock";
v0x647045d4bba0_0 .net "clock", 0 0, v0x647045d4b7b0_0;  alias, 1 drivers
v0x647045d952f0_0 .var "signal", 0 0;
E_0x647045d85400 .event posedge, v0x647045d4b7b0_0;
    .scope S_0x647045d49f50;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x647045d4b7b0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x647045d49f50;
T_1 ;
    %delay 12, 0;
    %load/vec4 v0x647045d4b7b0_0;
    %inv;
    %store/vec4 v0x647045d4b7b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x647045d950b0;
T_2 ;
    %wait E_0x647045d85400;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x647045d952f0_0, 0, 1;
    %delay 48, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x647045d952f0_0, 0, 1;
    %delay 48, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x647045d952f0_0, 0, 1;
    %delay 48, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x647045d952f0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x647045d49dc0;
T_3 ;
    %vpi_call 2 28 "$dumpfile", "Guia_0903.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000001, v0x647045d953f0_0, v0x647045d954e0_0 {0 0 0};
    %vpi_call 2 30 "$monitor", "clock=%b / p = %b ", v0x647045d953f0_0, v0x647045d954e0_0 {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
