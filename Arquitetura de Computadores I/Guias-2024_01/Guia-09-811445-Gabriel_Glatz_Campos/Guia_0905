#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x654f762cedf0 .scope module, "Guia_0905" "Guia_0905" 2 19;
 .timescale 0 0;
v0x654f76319a70_0 .net "clock", 0 0, v0x654f762d07b0_0;  1 drivers
v0x654f76319b60_0 .net "p", 0 0, v0x654f76319970_0;  1 drivers
S_0x654f762cef80 .scope module, "clk" "clock" 2 23, 3 4 0, S_0x654f762cedf0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "clk";
v0x654f762d07b0_0 .var "clk", 0 0;
S_0x654f76319730 .scope module, "pls" "pulse1" 2 24, 2 7 0, S_0x654f762cedf0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "signal";
    .port_info 1 /INPUT 1 "clock";
v0x654f762d0ba0_0 .net "clock", 0 0, v0x654f762d07b0_0;  alias, 1 drivers
v0x654f76319970_0 .var "signal", 0 0;
E_0x654f76309bc0 .event posedge, v0x654f762d07b0_0;
    .scope S_0x654f762cef80;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x654f762d07b0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x654f762cef80;
T_1 ;
    %delay 12, 0;
    %load/vec4 v0x654f762d07b0_0;
    %inv;
    %store/vec4 v0x654f762d07b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x654f76319730;
T_2 ;
    %wait E_0x654f76309bc0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x654f76319970_0, 0, 1;
    %delay 6, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x654f76319970_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x654f762cedf0;
T_3 ;
    %vpi_call 2 27 "$dumpfile", "Guia_0905.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x654f762cedf0 {0 0 0};
    %vpi_call 2 29 "$monitor", "clock=%b / p=%b ", v0x654f76319a70_0, v0x654f76319b60_0 {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Guia_0905.v";
    "./clock.v";
